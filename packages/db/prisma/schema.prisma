generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id           String    @id @default(cuid())
  name         String
  targetUrl    String
  ignoredPaths String[]  @default([])
  userId       String
  scriptPath   String?
  createdAt    DateTime  @default(now())
}
